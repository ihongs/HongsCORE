<?xml version="1.0" encoding="UTF-8"?>
<root>
    <form name="sign">
        <field name="username" required="yes">
        </field>
        <field name="password" required="yes">
        </field>
        <field name="captcode" required="yes" rule="Capts">
        </field>
    </form>
    <form name="mine">
        <field name="username" required="yes">
        </field>
        <field name="password" required="yes">
        </field>
        <field name="passolde">
        </field>
        <field name="name" required="yes">
        </field>
        <field name="head" rule="Thumb">
            <param name="href">public/upload/member/head</param>
            <param name="path">public/upload/member/head</param>
            <param name="thumb-extn">png</param>
            <param name="thumb-mode">pick</param>
            <param name="thumb-size">_lg:128*128;_md:64*64;_sm:32*32</param>
        </field>
    </form>
    <enum name="mine:preset">
        <value code=":defense">
            <![CDATA[{
                "id": ($session.uid)
            }]]>
        </value>
    </enum>
    <form name="user">
        <field name="head" rule="Thumb">
            <param name="href">upload/member/head</param>
            <param name="path">upload/member/head</param>
        </field>
        <field name="password">
            <param name="defiant"></param>
        </field>
    </form>
</root>
